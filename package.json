{
  "name": "ngx-localstorage-demo",
  "version": "3.4.0",
  "description": "An Angular wrapper for local storage access.",
  "scripts": {
    "ng": "ng",
    "update": "ng update @nrwl/workspace",
    "build:prod": "npm run build:lib && npm run copy && npm run build:docs",
    "build:lib": "ng build ngx-localstorage --configuration production",
    "copy": "npm run schematics:copy && copyfiles ./LICENSE ./README.md ./dist/libs/ngx-localstorage",
    "schematics:copy": "copyfiles -u 2 ./schematics/src/**/*.js ./schematics/src/**/*.json ./dist/libs/ngx-localstorage/schematics",
    "build:docs": "ng build --configuration production --output-path docs --base-href /ngx-localstorage/",
    "schematics:watch": "cd ./schematics/ && npm link && cd .. && npm link schematics && npm run build --prefix ./schematics -- --watch",
    "nx": "nx",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js",
    "lint": "ng lint",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "contributors:add": "all-contributors add",
    "contributors:check": "all-contributors check",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@nrwl/angular": "10.2.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-devkit/core": "^12.2.18",
    "@angular-devkit/schematics": "^12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "@nrwl/workspace": "10.2.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "all-contributors-cli": "^6.19.0",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.1.1",
    "dotenv": "6.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.7",
    "prettier": "^2.1.2",
    "protractor": "^7.0.0",
    "puppeteer": "^5.3.1",
    "ts-node": "~7.0.0",
    "tslint": "6.1.3",
    "typescript": "~4.3.5",
    "webpack": "^4.39.2"
  }
}